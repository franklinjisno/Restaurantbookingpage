@model Restaurantbookingpage.Booking
@using BookingClass.Enum;
@{

}
<form name="bookingdeleteForm">
    <div class="modal-header" style="padding-left:28%">
        <h4 class="modal-title">
            <span><b>Delete Booking</b></span>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        </h4>
    </div>
    <div class="modal-body" >
        @Html.HiddenFor(c => c.Id)
        <div class="row">
            <div class="col-md-10">
                <div class="form-group">
                    <b>Delete the booking of  @Html.DisplayFor(model => model.Customer_Name, new { htmlAttributes = new { @class = "form-control" } }) ? </b>
                </div>
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-sm pull-right" data-dismiss="modal">Cancel</button>
        <button type="button" onclick="Deletebooking()" style="padding-left: 24px;margin-right: 8px;margin-bottom: -84px;margin-top: 0px;" class="btn btn-sm btn-success pull-right">Delete</button>
    </div>
</form>

