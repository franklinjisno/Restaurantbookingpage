@using System.Web.Optimization;
<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Restaurant Booking</title>

    @Styles.Render("~/Content/CSS")

    <link href="~/Content/DataTables/css/responsive.dataTables.min.css" rel="stylesheet" />
    @Scripts.Render("~/Scripts/Script")
    <script src="~/Scripts/DataTables/dataTables.responsive.min.js"></script>

</head>
<body style= "background-color:#ffffff";>
    <div class="navbar navbar-inverse navbar-static-top" style="background-color: antiquewhite">
        <div class="container-fluid" style="text-align: center; background-color: #1d8b96;">

            <h1><b>RESTAURANT BOOKING</b></h1>
            @if(Request.IsAuthenticated)
            {
                <strong style="margin-left: -1365px;">@Html.Encode(User.Identity.Name) </strong>
            }

        </div>
    </div>

    
    <div class="container body-content" style="font-size:15px">
        @RenderBody()


    </div>


    <div class="container body-content">

        

            
    </div>


</body>
</html>