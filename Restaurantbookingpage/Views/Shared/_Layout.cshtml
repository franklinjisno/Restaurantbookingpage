@using System.Web.Optimization;
<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Restaurant Booking</title>

    @Styles.Render("~/Content/CSS")
    @Scripts.Render("~/Scripts/Script")
  
</head>
<body style="background-color:antiquewhite">
    <div class="navbar navbar-inverse navbar-static-top" style="background-color: antiquewhite">
        <div class="container-fluid" style="text-align: center">

            <h1><b>Restaurant Booking</b></h1>

        </div>
    </div>
   
    <div class="container body-content" style="font-size:15px">
        @RenderBody()
     

    </div>


    <div class="container body-content">

        <hr />

        <div>
        
            <footer class="footer navbar-sticky-bottom" style="align-items:center">
                <p>&copy; @DateTime.Now.Year - Restaurant Booking</p>
            </footer>
        </div>

    </div>
</body>
</html>